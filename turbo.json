{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"]
		},
		"start": {
			"dependsOn": ["^start"]
		},
		"@gameday/client#build": {
			"outputs": ["../api/public/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
